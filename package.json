  },
  "devDependencies": {
    "@remix-run/dev": "2.9.2",
    "@remix-run/eslint-config": "2.9.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "npm-run-all": "4.1.5",

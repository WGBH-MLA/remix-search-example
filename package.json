    "@searchkit/instantsearch-client": "^4.14.0",
    "algoliasearch": "5.2.5",
    "instantsearch.css": "8.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-instantsearch": "7.13.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.11.2",
    "@remix-run/eslint-config": "2.11.2",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "npm-run-all": "4.1.5",
    "tailwindcss": "3.4.10",
